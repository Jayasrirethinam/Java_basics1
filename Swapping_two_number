package Jaya;

public class Swappingwith3variable {
static void swapping1() {

	int a=10;
	int b=20;
	a=a+b;
	b=a-b;
	a=a-b;
	System.out.println(a);
	System.out.println(b);
	
}
static void swapping2() {

	int a=10;
	int b=20;
	int c;
	c=b;
	b=a;
	a=c;
	System.out.println(a);
	System.out.println(b);
	
}
	public static void main(String[] args) {
		
		//swapping1();
		//swapping2();
		
	}

}
